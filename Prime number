#include<stdio.h>
Main()
{
 int n,i,sum=0;
 Printf("enter a pistive intrger");
 scanf("%d",&n);
 for(i=2;i<=n/2;++i);
 {
  if(n%i==0)
 {
  sum=1;
  break;
 }
}
 if(sum==0)
 print("%dis prime number",n);
 else
 Printf("%d is Not a prime number",n);
}

Output:
Enter a positive number23
23 is a prime number

